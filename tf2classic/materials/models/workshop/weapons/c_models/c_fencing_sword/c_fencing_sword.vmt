"VertexlitGeneric"
{
	"$baseTexture" "models\props_mac/prop_hiltedsword"
	"$bumpmap" "models\props_mac/prop_hiltedsword_normal"


	"$phong" "1"
	"$phongexponenttexture" "models\props_mac/prop_hiltedsword_exp"
	"$phongexponent" "0"
	"$phongboost" "3"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
//	"$phongwarptexture"  "models\weapons\c_items/c_ambassador_phongwarp"
	"$envmap" "env_cubemap"
//	"$basealphaenvmapmask" "1"
//	"$normalmapalphaphongmask" "1"


	"$rimlight" "1"
	"$rimlightexponent" "6"	
	"$rimlightboost" "3"

	"360?$color2" "[ 0.9 0.8 0.8 ]"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}