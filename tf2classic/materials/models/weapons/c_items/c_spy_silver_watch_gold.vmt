"VertexlitGeneric"
{
	///////////////////////////////////////////////////////////////////////////////
	//					texture.
	///////////////////////////////////////////////////////////////////////////////
	$basetexture "models\weapons\c_items/c_spy_silver_watch_exterior.vtf"
	$model 1


	///////////////////////////////////////////////////////////////////////////////
	//					phong.
	///////////////////////////////////////////////////////////////////////////////
	$phong 1
	$phongboost 1
	$phongexponent 8
	$phongfresnelranges "[1 0.25 3]"
	$phongtint "[1 0.6 0.45]"

	$rimlight 1
	$rimlightexponent  4
	$rimlightboost 2

	$envmap "cubemaps\cubemap_specular001"
	$envmaptint "[1 0.50 0.25]"
	$envmapcontrast 0.25

	$lightwarptexture "models\weapons\c_items/c_ambassador_lightwarp"


	///////////////////////////////////////////////////////////////////////////////
	//					paint section.	
	///////////////////////////////////////////////////////////////////////////////
	//$BlendTintByBaseAlpha 1
	$BlendTintColorOverBase 1.0

	$colortint_base "{195 160 120}"
	$colortint_tmp "[0 0 0]"
	$color2 "{195 160 120}"

	$BaseMapAlphaPhongMask 1
	$BaseAlphaEnvMapMask 1


	///////////////////////////////////////////////////////////////////////////////
	//					special effects.
	///////////////////////////////////////////////////////////////////////////////
	$yellow 0
	$glowcolor 1

	$cloakPassEnabled 1
	$sheenPassEnabled 1

	$sheenmap		"cubemaps\cubemap_sheen001"
	$sheenmapmask		"Effects\AnimatedSheen\animatedsheen0"
	$sheenmaptint		"[ 1 1 1 ]"
	$sheenmapmaskframe 	0
	$sheenindex 		0


	///////////////////////////////////////////////////////////////////////////////
	//					Proxies.
	///////////////////////////////////////////////////////////////////////////////
	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
  		"ItemTintColor"
  		{
   			"resultVar" "$colortint_tmp"
  		}
		"SelectFirstIfNonZero"
  		{
   			"srcVar1" "$colortint_tmp"
   			"srcVar2" "$colortint_base"
   			"resultVar" "$color2"
  		}
		//View Model Cloaking
		//"vm_invis"
		//{
		//}
	}
}
