"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\v_rocket\rocket"
	"$bumpmap" "models\weapons\v_rocket\rocket_normal"
	"$normalmapalphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.05 .05 .05 ]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "3"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.2 .5 1]"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
