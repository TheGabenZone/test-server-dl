"VertexlitGeneric"
{
	"$basetexture" "vgui/replay/thumbnails/models/weapons/c_models/c_stickybomb_launcher/tf_sticky_02"
		

	"$teamcolors" "1"

		 "$phong" "1"
		 "$phongexponent" "2"
		 "$phongboost" "4"	
	     "$lightwarptexture" "models/lightwarps\weapon_lightwarp"
         "$phongfresnelranges"	"[.1 2 .1]"
        "$bumpmap" "vgui/replay/thumbnails/models/weapons/c_models/c_stickybomb_launcher/tf_sticky_nrm"

    $selfillum 1
    $selfillummask "vgui/replay/thumbnails/models/weapons/c_models/c_stickybomb_launcher/tf_sticky_mask"
    $selfillumtint "[1 1 1]"

	"$phongExponentTexture" "vgui/replay/thumbnails/models/weapons/c_models/c_stickybomb_launcher/tf_sticky_specular_01"
	$phongAlbedoTint 1
	
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "7"
	"$rimmask" "1"

	"$envmap" "env_cubemap"	
	"$envmaptint" "[0.7 0.7 0.7]"


"$glowcolor" "1"


"$tempvar" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}
